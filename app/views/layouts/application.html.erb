  <%= javascript_include_tag 'https://d3js.org/d3.v4.js' %>
  <%= javascript_include_tag 'https://d3js.org/d3-axis.v1.min.js' %>
  <%= javascript_include_tag "https://api.tiles.mapbox.com/mapbox.js/v2.2.4/mapbox.js" %>
